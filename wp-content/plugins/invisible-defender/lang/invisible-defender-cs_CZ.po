#: invisible-defender.php:79
msgid "Please leave these two fields as-is:"
msgstr "Nechte následující pole tak, jak jsou, prosím:"

#: invisible-defender.php:85
#, php-format
msgid "Protected by %s."
msgstr "Chráněno %s."

#: invisible-defender.php:94
#, php-format
msgid "Showed <strong>403</strong> to <strong>%s</strong> bad guys."
msgstr "Hlavička <strong>403</strong> byla ukázána <strong>%s</strong> zlým maníkům."

#: invisible-defender.php:173
#, php-format
msgid "<strong>Invisible Defender</strong> showed <strong>403</strong> to <strong>%1$s</strong> bad guys (<strong>%2$s</strong> was blacklisted, <strong>%3$s</strong> on register form, <strong>%4$s</strong> on login form, <strong>%5$s</strong> on comments form)."
msgstr "<strong>Invisible Defender</strong> ukázal hlavičku 403 <strong>%1$s</strong> zlým maníkům (<strong>%2$s</strong> bylo na černé listině, <strong>%3$s</strong> na registračním formuláři, <strong>%4$s</strong> na přihlašovacím formuláři, <strong>%5$s</strong> na komentovacím formuláři)."

#: invisible-defender.php:190
msgid "Configuration has been saved."
msgstr "Konfigurace byla uložena."

#: invisible-defender.php:195
msgid "Invisible Defender - Options"
msgstr "Invisible Defender - Nastavení"

#: invisible-defender.php:201
msgid "Forms protection:"
msgstr "Ochrana formulářů:"

#: invisible-defender.php:205
msgid "Protect registration form:"
msgstr "Chránit registrační formulář:"

#: invisible-defender.php:214
msgid "Protect login form:"
msgstr "Chránit přihlašovací formulář:"

#: invisible-defender.php:223
msgid "Protect comments form:"
msgstr "Chránit komentovací formulář:"

