#, php-format
msgid "Protected by %s."
msgstr "Védelmet biztosítja %s."

#: invisible-defender.php:96
#, php-format
msgid "Showed <strong>403</strong> to <strong>%s</strong> bad guys."
msgstr "<strong>403</strong> megjelenítve az <strong>%s</strong> spammereknek."

#: invisible-defender.php:163
#, php-format
msgid "<strong>Invisible Defender</strong> showed <strong>403</strong> to <strong>%1$s</strong> bad guys (<strong>%2$s</strong> was blacklisted, <strong>%3$s</strong> on register form, <strong>%4$s</strong> on login form, <strong>%5$s</strong> on comments form)."
msgstr "<strong>Invisible Defender</strong> által <strong>403</strong> megjelenítve<strong>%1$s</strong> spammernek (<strong>%2$s</strong> feketelistára került, <strong>%3$s</strong> a regisztráció során, <strong>%4$s</strong> a bejelentkezés során, <strong>%5$s</strong> hozzászólás során)."

