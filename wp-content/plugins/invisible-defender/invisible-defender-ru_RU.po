#: invisible-defender.php:81
msgid "Please leave these two fields as-is:"
msgstr "Оставьте эти два поля как есть:"

#: invisible-defender.php:87
#, php-format
msgid "Protected by %s."
msgstr "Защищено %s."

#: invisible-defender.php:96
#, php-format
msgid "Showed <strong>403</strong> to <strong>%s</strong> bad guys."
msgstr "Показывать <strong>403</strong> для <strong>%s</strong> плохих парней."

#: invisible-defender.php:163
#, php-format
msgid "<strong>Invisible Defender</strong> showed <strong>403</strong> to <strong>%1$s</strong> bad guys (<strong>%2$s</strong> was blacklisted, <strong>%3$s</strong> on register form, <strong>%4$s</strong> on login form, <strong>%5$s</strong> on comments form)."
msgstr "<strong>Invisible Defender</strong> показал <strong>403</strong> для <strong>%1$s</strong> плохих парней (<strong>%2$s</strong> было занесено в черный список, <strong>%3$s</strong> в форме регистрации, <strong>%4$s</strong> в форме для входа, <strong>%5$s</strong> в форме комментариев."

#: invisible-defender.php:182
msgid "Configuration has been saved."
msgstr "Конфигурация была сохранена."

#: invisible-defender.php:188
msgid "Invisible Defender - Options"
msgstr "Invisible Defender - Настройки"

#: invisible-defender.php:194
msgid "Forms protection:"
msgstr "Защита форм:"

#: invisible-defender.php:198
msgid "Protect registration form:"
msgstr "Защита формы регистрации:"

#: invisible-defender.php:207
msgid "Protect login form:"
msgstr "Защита формы для входа:"

#: invisible-defender.php:216
msgid "Protect comments form:"
msgstr "Защита формы для комментариев:"

#: invisible-defender.php:223
msgid "Stats and \"Protected by\" link:"
msgstr "Статистика и ссылка \"Защищено\":"

